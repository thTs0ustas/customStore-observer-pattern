{
	"presets": [
		[
			"@babel/preset-typescript",
			{
				"isTSX": true,
				"allExtensions": true
			}
		],
		[
			"@babel/preset-react",
			{ "runtime": "automatic", "importSource": "react" }
		],
		[
			"@babel/preset-env",
			{
				"useBuiltIns": "usage",
				"corejs": "3.25.2",
				"targets": [">0.25%", "not dead"]
			}
		]
	]
}
